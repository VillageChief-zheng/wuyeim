<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:imagecontrol="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
     android:background="#FF191725"
    >
<RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="@dimen/dap70"
  android:background="#FF2B2938"
    android:layout_marginLeft="@dimen/pad_10"
    android:layout_marginRight="@dimen/pad_10"
    android:layout_marginTop="@dimen/dap80"
    android:gravity="center_vertical"
    android:id="@+id/rl_musci"
    android:visibility="gone"

    >
<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:src="@mipmap/ic_ks"
    android:layout_marginLeft="@dimen/pad_20"
    android:id="@+id/im_ztandks"
    android:adjustViewBounds="true"
    android:scaleType="fitXY"
       />
<SeekBar
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_toRightOf="@+id/im_ztandks"
    android:id="@+id/seekbar"
    />
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/seekbar"
        android:layout_toRightOf="@+id/im_ztandks"
        android:layout_marginTop="@dimen/dap5"

        >
<TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:textColor="#FF999999"
    android:textSize="@dimen/sp_10"
    android:text=""
    android:layout_marginLeft="@dimen/dap10"
    android:id="@+id/start"
    />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#FF999999"
            android:textSize="@dimen/sp_10"
            android:text=""
            android:layout_marginLeft="@dimen/dap10"
            android:id="@+id/end"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/dap10"
            />

    </RelativeLayout>

</RelativeLayout>
    <Button
        android:layout_width="match_parent"
        android:layout_height="@dimen/dap50"
        android:text="发送"
        android:background="@drawable/fillet_grbg"
        android:textColor="@color/white"
        android:textSize="@dimen/textsp18"
        android:layout_marginTop="@dimen/dap40"
        android:layout_marginLeft="@dimen/dap26"
        android:layout_marginRight="@dimen/dap26"
        android:id="@+id/bt_submit"
        android:layout_below="@+id/rl_musci"
        android:visibility="gone"
        />
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="300dp"
          android:layout_alignParentBottom="true"
        android:layout_marginBottom="@dimen/pad_100"
        android:layout_centerVertical="true"
        >
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="350dp" >
            <com.wuye.piaoliuim.utils.WhewView
                android:id="@+id/wv"
                android:layout_width="match_parent"
                android:layout_height="350dp" />
            <com.wuye.piaoliuim.utils.RoundImageView
                android:id="@+id/my_photo"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_centerInParent="true"
                android:src="@mipmap/ic_yy_not"
                imagecontrol:border_inside_color="#FF221F32"
                imagecontrol:border_outside_color="#FF221F32"
                imagecontrol:border_thickness="1dp" />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/my_photo"
                android:gravity="center"
                android:text="重新录取声音"
                android:textColor="#FF999999"
                android:textSize="@dimen/sp_15"
                android:layout_marginTop="@dimen/dap30"
                android:id="@+id/restyuyim"
                android:visibility="gone"
                />
        </RelativeLayout>


    </RelativeLayout>
</RelativeLayout>
